			"type": "string",
			"defaultValue": "https://storageaccalan.dfs.core.windows.net/"
		},
		"AzureSqlDatabase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "trainingserv11.database.windows.net"
		},
		"AzureSqlDatabase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "training_DB"
		},
		"AzureSqlDatabase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"SuhailsDB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "trainserver.database.windows.net"
